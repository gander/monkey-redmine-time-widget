{
  "name": "@gander/monkey-redmine-time-widget",
  "description": "Redmine Widget for total dev time presentation",
  "author": "Adam GÄ…sowski",
  "private": true,
  "version": "5.0.0",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitArgs": [
        "-S"
      ]
    },
    "github": {
      "release": true,
      "draft": true,
      "assets": [
        "dist/**/*.js"
      ],
      "releaseName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": [
        "yarn build",
        "yarn auto-changelog --package --commit-limit false --template keepachangelog --hide-credit"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "release": "dotenv -c production release-it --"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.0",
    "pinia": "^2.1.4",
    "primevue": "^3.29.2",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "auto-changelog": "^2.4.0",
    "dotenv-cli": "^7.2.1",
    "release-it": "^15.11.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-monkey": "^3.2.2",
    "vue-tsc": "^1.8.1"
  }
}
